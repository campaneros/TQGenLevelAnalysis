using namespace std;



int run(){
  gRoot->ProcessLine(".L Analysis.cc++");
  TFile* f= new TFile("../../crab/ntuple_tetraquarks_wLowPt.root");
  TTree* t = (TTree*) f->Get("GenAnalysis/tree");

  //================ Run analysis
  Analysis ana( t );
  t.Loop();
  
  delete f;
  delete t;

  return 0;
}
