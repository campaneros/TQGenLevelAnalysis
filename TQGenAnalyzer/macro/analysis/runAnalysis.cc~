#include <stdlib.h>
#include <iostream>
#include <fstream>
#include <string>

#include <TTree.h>
#include <TChain.h>
#include <TROOT.h>
#include <TChain.h>
#include <TFile.h>
#include <TH2.h>
#include <TH1F.h>
#include <TStyle.h>
#include <TCanvas.h>
#include <TVector2.h>
#include <TVector3.h>
#include <TAxis.h>

#include "TQAnalysis.h"

using namespace std;



void run(){

  TFile* fin= new TFile("/eos/cms/store/group/phys_egamma/soffi/ggXToYYTo2mu2e/ntuple_ggXToYYTo2mu2e_m26_PseudoScalar_13TeV_106X_PRIVATE_MATTIA_100K.root");
  TTree* chain =(TTree*) fin->Get("GenAnalysis/tree");

  std::cout<<chain->GetEntries()<<std::endl;      
  //================ Run analysis
  TQAnalysis tree( chain );
  tree.Loop();
  
  delete fin;
  delete chain;
  return 0;
}
